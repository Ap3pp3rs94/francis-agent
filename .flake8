[flake8]
max-line-length = 88
extend-ignore = E203, W503
extend-exclude =
    .venv,
    .venv-*,
    venv,
    dist,
    build,
    .mypy_cache,
    .pytest_cache,
    *.egg-info,
    **/site-packages/*,
    tools,
    backfill_mem_index.py,
    francis.backup.py,
    long_memory.py,
    reflection_memory.py
